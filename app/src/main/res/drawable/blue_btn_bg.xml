<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/cs_btn_blue_press" />
            <!--<stroke android:width="0dp" android:color="#488484" android:dashGap="0dp" android:dashWidth="0dp" />-->
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <padding android:bottom="5dp" android:left="15dp" android:right="15dp" android:top="5dp" />

        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/cs_btn_blue" />
            <!--<stroke android:width="0dp" android:color="#488484" android:dashGap="0dp" android:dashWidth="0dp" />-->
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <padding android:bottom="5dp" android:left="15dp" android:right="15dp" android:top="5dp" />

        </shape>
    </item>

</selector>